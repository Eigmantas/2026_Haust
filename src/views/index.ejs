<%- include('partials/header') %>
<main>
  <h1>Vinsælar myndir</h1>
  <div class="game">
    <% games.forEach(game => { %>
      <div class="game">
        <img class ="image" src="<%= game.image %>" alt="<%= game.title %>">
        <h3><%= game.title %></h3>
        <p><%= game.year %></p>
        <a href="/game/<%= game.id %>" class="btn">Skoða nánar</a>
      </div>
    <% }) %>
  </div>
</main>
<%- include('partials/footer') %>